{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "commonjs": true
  },
  "plugins": ["import", "react", "jsx-a11y", "react-hooks"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/extensions": [".js", ".jsx"],
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx"]
      }
    }
  },
  "rules": {
    "no-console": 1,
    "no-eval": 2,
    "import/first": 2,
    "semi": [2, "always"],
    "semi-spacing": 2,
    "semi-style": 2,
    "no-extra-semi": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-string-refs": 0,
    "react/prop-types": 0,
    "quotes": [
      2,
      "double",
      {
        "avoidEscape": true
      }
    ],
    "jsx-quotes": [2, "prefer-double"]
  }
}
